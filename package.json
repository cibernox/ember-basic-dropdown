{
  "name": "ember-basic-dropdown",
  "version": "6.0.2",
  "description": "The basic dropdown that your ember app needs",
  "homepage": "http://ember-basic-dropdown.com",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "files": [
    "addon/",
    "app/",
    "addon-test-support/",
    "test-support/",
    "index.js",
    "vendor"
  ],
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.5.0",
    "@embroider/test-setup": "^1.0.0",
    "@types/ember": "^3.16.0",
    "@types/ember-qunit": "^3.4.15",
    "@types/ember__destroyable": "^4.0.0",
    "@types/ember__test-helpers": "^0.7.9",
    "@types/qunit": "^2.11.2",
    "@types/rsvp": "^4.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.0",
    "ember-cli": "~3.28.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deprecation-workflow": "^2.1.0",
    "ember-cli-fastboot": "^3.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-memory-leak-detector": "^0.7.1",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-code-example-component": "^0.3.1",
    "ember-code-snippet": "2.3.1",
    "ember-concurrency": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "^3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^7.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prember": "~1.1.0",
    "prember-crawler": "^1.0.0",
    "prettier": "^2.3.2",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "sass": "^1.43.5",
    "simple-git": "^3.3.0",
    "typescript": "^4.2.0",
    "webpack": "^5.0.0"
  },
  "keywords": [
    "ember-addon",
    "dropdown",
    "basic-dropdown"
  ],
  "repository": "https://github.com/cibernox/ember-basic-dropdown",
  "license": "MIT",
  "author": "Miguel Camba",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.4",
    "@embroider/macros": "^1.2.0",
    "@embroider/util": "^1.2.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-typescript": "^4.2.1",
    "ember-element-helper": "^0.6.0",
    "ember-get-config": "^2.1.1",
    "ember-maybe-in-element": "^2.0.3",
    "ember-modifier": "^3.2.7",
    "ember-style-modifier": "^0.8.0 || ^1.0.0",
    "ember-truth-helpers": "^2.1.0 || ^3.0.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "16.14.2"
  }
}
