{
  "name": "ember-basic-dropdown",
  "version": "3.0.16",
  "description": "The basic dropdown that your ember app needs",
  "homepage": "http://ember-basic-dropdown.com",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "files": [
    "addon/",
    "app/",
    "addon-test-support/",
    "test-support/",
    "index.js",
    "vendor"
  ],
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.0",
    "@types/ember": "^3.1.1",
    "@types/ember-qunit": "^3.4.7",
    "@types/ember__test-helpers": "^0.7.9",
    "@types/qunit": "^2.9.0",
    "@types/rsvp": "^4.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.25.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deprecation-workflow": "^1.0.1",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-memory-leak-detector": "^0.5.1",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-terser": "^4.0.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-code-example-component": "^0.3.1",
    "ember-code-snippet": "2.3.1",
    "ember-concurrency": "^1.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-href-to": "~3.1.0",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.2",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.25.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "loader.js": "^4.7.0",
    "node-sass": "*",
    "npm-run-all": "^4.1.5",
    "prember": "~1.0.0",
    "prember-crawler": "^1.0.0",
    "prettier": "^2.2.1",
    "qunit": "^2.14.0",
    "qunit-dom": "^1.6.0",
    "simple-git": "^1.10.0",
    "typescript": "^4.2.0"
  },
  "keywords": [
    "ember-addon",
    "dropdown",
    "basic-dropdown"
  ],
  "repository": "https://github.com/cibernox/ember-basic-dropdown",
  "license": "MIT",
  "author": "Miguel Camba",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "@embroider/macros": "^0.36.0",
    "@embroider/util": "^0.36.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "ember-cli-babel": "^7.23.1",
    "ember-cli-htmlbars": "^5.3.2",
    "ember-cli-typescript": "^4.1.0",
    "ember-element-helper": "^0.3.2",
    "ember-maybe-in-element": "^2.0.2",
    "ember-style-modifier": "^0.6.0",
    "ember-truth-helpers": "^2.1.0 || ^3.0.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
